{
   "author": "Sainan",
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "discord.js" ],
      "matches": [ "https://discord.com/channels/*", "https://ptb.discord.com/channels/*", "https://canary.discord.com/channels/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "youtube.js", "content_script.js" ],
      "matches": [ "https://www.youtube.com/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": true,
      "js": [ "youtubemusic.js", "content_script.js" ],
      "matches": [ "https://music.youtube.com/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": true,
      "js": [ "soundcloud.js", "content_script.js" ],
      "matches": [ "https://soundcloud.com/*" ],
      "run_at": "document_idle"
   }, {
      "all_frames": true,
      "js": [ "plex.js", "content_script.js" ],
      "matches": [ "https://app.plex.tv/desktop", "https://app.plex.tv/desktop/*" ],
      "run_at": "document_idle"
   } ],
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "41",
   "name": "__MSG_appName__",
   "permissions": [ "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.10.0"
}
