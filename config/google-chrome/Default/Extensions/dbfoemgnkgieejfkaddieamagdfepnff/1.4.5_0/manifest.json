{
   "action": {
      "default_icon": {
         "128": "images/icons/icon128.png",
         "16": "images/icons/icon16.png",
         "32": "images/icons/icon32.png",
         "48": "images/icons/icon48.png",
         "96": "images/icons/icon96.png"
      }
   },
   "author": "Two Factor Authentication Service, Inc.",
   "background": {
      "service_worker": "background/background.js"
   },
   "commands": {
      "tokenRequest": {
         "description": "__MSG_shortcutDesc__",
         "global": false,
         "suggested_key": {
            "default": "Ctrl+Shift+2",
            "mac": "MacCtrl+Shift+2"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content/content_script.js" ],
      "match_about_blank": true,
      "matches": [ "https://*/*", "http://*/*" ]
   }, {
      "all_frames": false,
      "css": [ "content/content_script.css" ],
      "match_about_blank": true,
      "matches": [ "https://*/*", "http://*/*" ]
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "host_permissions": [ "*://*.2fas.com/*" ],
   "icons": {
      "128": "images/icons/icon128.png",
      "16": "images/icons/icon16.png",
      "32": "images/icons/icon32.png",
      "48": "images/icons/icon48.png",
      "96": "images/icons/icon96.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "2FAS - Two Factor Authentication",
   "options_ui": {
      "open_in_tab": true,
      "page": "optionsPage/optionsPage.html"
   },
   "permissions": [ "activeTab", "tabs", "storage", "notifications", "contextMenus" ],
   "short_name": "2FAS",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4.5",
   "web_accessible_resources": [ {
      "matches": [ "https://*/*", "http://*/*" ],
      "resources": [ "/fonts/*" ]
   } ]
}
